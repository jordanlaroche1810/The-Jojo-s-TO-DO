generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:@localhost:3306/todo"
}

model Todo {
  id          Int       @id @default(autoincrement())
  nom         String?
  statut      String?
  description String?
  completed   Boolean   @default(false)
  dueDate     String?
}
